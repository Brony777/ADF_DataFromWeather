{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AdfWeathertb"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'",
			"defaultValue": "DefaultEndpointsProtocol=https;AccountName=smweather;EndpointSuffix=core.windows.net;"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"SandboxAzure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SandboxAzure'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"stagerawdata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'stagerawdata'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1101-200746-l7lbyi4n"
		},
		"GiosRestApi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.gios.gov.pl/pjp-api/rest/"
		},
		"azk_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://tobepasswords.vault.azure.net/"
		},
		"stagerawdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://smweather.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AdfWeathertb_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"SandboxAzure_connectionString": {
						"value": "[parameters('SandboxAzure_connectionString')]"
					},
					"stagerawdata_accountKey": {
						"value": "[parameters('stagerawdata_accountKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"GiosRestApi_properties_typeProperties_url": {
						"value": "[parameters('GiosRestApi_properties_typeProperties_url')]"
					},
					"azk_properties_typeProperties_baseUrl": {
						"value": "[parameters('azk_properties_typeProperties_baseUrl')]"
					},
					"stagerawdata_properties_typeProperties_url": {
						"value": "[parameters('stagerawdata_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AdfWeathertb_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AdfWeathertb_ArmTemplate_0')]"
			]
		}
	]
}